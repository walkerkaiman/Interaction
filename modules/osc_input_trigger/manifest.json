{
  "name": "OSC Input",
  "type": "input",
  "classification": "trigger",
  "mode": "trigger",
  "description": "Receives OSC messages and emits events",
  "fields": [
    {
      "name": "ip_address",
      "type": "label",
      "label": "All Interfaces",
      "default": "0.0.0.0 (All Interfaces)"
    },
    {
      "name": "port",
      "type": "text",
      "label": "Port",
      "default": 8000
    },
    {
      "name": "address",
      "type": "text",
      "label": "OSC Address",
      "default": "/trigger"
    },
    {
      "name": "reset",
      "type": "button",
      "label": "Reset"
    }
  ]
}
